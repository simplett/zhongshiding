/div>\n          <div v-if=\"item.isshow\">\n            <a style=\"color:#ffffff,font-family:'微软雅黑'\" href=\"javascript();\">了解更多</a>\n          </div>\n        </div>\n      </el-col>\n    </el-row>\n  </div>\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      Ifon: [\n        {\n          name: \"first\",\n          image: null,\n          title1: \"开辟\",\n          title2: \"创新之路\",\n          title3: \"大事记\",\n          msg:\n            \"中视典自2002年创立以来一直致力于中国虚拟现实技术自有软件的开发和探索\"\n        },\n        {\n          name: \"second\",\n          image: require(\"../../assets/images/fzlc_two.png\"),\n          isshow: true,\n          title1: \"为技术创新\",\n          title2: \"助一臂之力\",\n          title3: \"资质荣誉\",\n          msg:\n            \"近20年的创新：从国内第一个虚拟现实平台软件VRPlatform到今天的一站式数字解决方案提供商\"\n        },\n        {\n          name: \"third\",\n          image: null,\n          title1: \"用心\",\n          title2: \"累积成长\",\n          title3: \"客户认可\",\n          msg:\n            \"已经为分布在世界各地的5万多家客户带去了价值超过1000所大专院校和科研所成为了中视典的合作伙伴\"\n        }\n      ]\n    };\n  },\n  methods: {\n    changeActivename(name) {\n      this.$store.commit(\"getActiveName\", name);\n      this.$store.commit(\"setshowmytab\", false);\n    },\n    ChangeBg(index) {\n      console.log(index);\n      this.Ifon.forEach((item, i) => {\n        if (i !== index) {\n          this.Ifon[i].image = null;\n          console.log(\"this is background change\");\n          this.$set(this.Ifon, i, this.Ifon[i]);\n        }\n      });\n      var image = require(\"../../assets/images/fzlc_two.png\");\n      this.Ifon[index].image = image;\n    }\n  }\n};\n</script>\n<style scoped>\n.w-100 {\n  width: 100%;\n}\n.first {\n  font-size: 50px;\n  color: #999999;\n  font-weight: 800;\n  font-family: \"微软雅黑\";\n}\n.scend {\n  color: #999999;\n  font-size: 20px;\n  font-family: \"微软雅黑\";\n  font-weight: bold;\n  cursor: pointer;\n}\n.text {\n  width: 50px;\n  height: 0px;\n  border-bottom: 4px solid #fff;\n}\n.last {\n  color: #fff;\n  font-size: 25px;\n  font-family: \"微软雅黑\";\n  margin-left: 20px;\n  margin-right: 20px;\n}\n.mymsg {\n  font-family: \"微软雅黑\";\n  color: #ffffff;\n}\n.lyaot {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  flex-direction: column;\n  flex: content;\n  height: 700px;\n  color: #fff;\n  background-color: #202020;\n}\n.lyaot > div {\n  text-align: center;\n  margin: 15px auto;\n  padding: 2px 20px !important;\n  box-sizing: border-box;\n}\n/* .lyaot:nth-child(2) {\n  background-image: url(\"../../assets/images/fzlc_two.png\");\n} */\n</style>"]}]}